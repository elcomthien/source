<%@page contentType="text/html" pageEncoding="UTF-8"%>
<%@taglib prefix="s" uri="/struts-tags"%>

<!-- start info detail-->
<div class="info-detail">
	<div class="info-detail-left">
		<form action="" method="post" class="ui-form">
			<label> <span>IP :</span> <input id="name" type="text"
				name="ip" placeholder="IP" /> </label> <label> <span>Tên :</span> <input
				id="name" type="text" name="name" placeholder="Tên Player" /> </label>
		</form>
		<div style="margin-left: 10px;">
			<p1 class="text-title">Danh Sách Tập Tin:</p1>
			<div style="clear: both;">&nbsp;</div>
			<div class="playerofgroup">
				<table>
					<thead>
						<tr>
							<th>Tên</th>
							<th>Xóa</th>
						</tr>
					</thead>
					<tbody id="scrolling">
						<tr>
							<td>Video</td>
							<td style="text-align: center"><a href="#" class="ask">
									<img src="css/images/trash.png" alt="" title="" border="0">
							</a></td>
						</tr>
						<tr>
							<td>Image</td>
							<td style="text-align: center"><a href="#" class="ask">
									<img src="css/images/trash.png" alt="" title="" border="0">
							</a></td>
						</tr>
						<tr>
							<td>Text</td>
							<td style="text-align: center"><a href="#" class="ask">
									<img src="css/images/trash.png" alt="" title="" border="0">
							</a></td>
						</tr>
					</tbody>
				</table>
			</div>
		</div>
		<div class="btn-update">
			<form action="" method="post" class="ui-form">
				<label> <span>&nbsp;</span> <input type="submit"
					class="button" id="btnupdate" value="Cập Nhật" /> </label>
			</form>
		</div>
	</div>

	<div class="info-detail-right">
		<div>
			<p1 class="text-title">Trạng thái:</p1>
			<img src="css/images/validgreen.png" />
		</div>
		<div class="layout-detail"></div>
		<div class="chart">
			<div id="piechart-memory"
				style="width: 200px; height: 60px; float: left"></div>
			<div id="piechart-ram"
				style="width: 200px; height: 60px; float: left"></div>
		</div>
	</div>
</div>
<div style="clear: both;">&nbsp;</div>
<!-- end table detail-->
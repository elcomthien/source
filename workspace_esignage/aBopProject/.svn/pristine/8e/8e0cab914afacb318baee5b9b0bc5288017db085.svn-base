body {
	margin-top: 50px;
	padding: 0;
	background: #FFFFFF url(images/img01.jpg) repeat-x left top;
	font-family: "Trebuchet MS", Arial, Helvetica, sans-serif;
	font-size: 13px;
	color: #837669;
}

h1,h2,h3 {
	margin: 0;
	font: menu;
	font-weight: normal;
	color: #649632;
}

h1 {
	font-size: 44px;
}

h2 {
	font-size: 18px;
}

h3 {
	
}

p,ul,ol {
	margin-top: 0;
	line-height: 100%;
	text-align: justify;
	font-family: "Trebuchet MS", Arial, Helvetica, sans-serif;
	font-size: 11px;
}

ul,ol {
	
}

blockquote {
	
}

a {
	color: #000000;
}

a:hover {
	text-decoration: none;
}

a img {
	border: none;
}

img.left {
	float: left;
	margin: 7px 30px 0 0;
}

img.right {
	float: right;
	margin: 7px 0 0 30px;
}

hr {
	display: none;
}

.list1 {
	
}

.list1 li {
	float: left;
	line-height: normal;
}

.list1 li img {
	margin: 0 30px 30px 0;
}

.list1 li.alt img {
	margin-right: 0;
}

#wrapper {
	
}
/* Header */
#header-wrapper {
	width: 880px;
	margin: 0 auto;
}

#header {
	width: 900px;
	height: 45px;
	margin: 0 auto;
	/*background: url(images/img02.jpg) no-repeat left top;*/
	background-color: #006666;
	border-radius: 5px;
}

/* Menu */
#menu {
	float: left;
	width: 540px;
	height: 51px;
}

#menu ul {
	margin: 0;
	padding: 20px 0 0 20px;
	list-style: none;
	line-height: normal;
}

#menu li {
	display: block;
	float: left;
}

#menu a {
	display: block;
	float: left;
	margin-right: 5px;
	padding: 5px 12px 5px 28px;
	/*	background: url(images/img06.gif) no-repeat left 50%;*/
	text-decoration: none;
	font: 14px Georgia, "Times New Roman", Times, serif;
	color: #FFFFFF;
}

#menu a:hover {
	text-decoration: underline;
}

#menu .current_page_item a {
	color: #FFFFFF;
}
/* Search */
#search {
	float: right;
	width: 305px;
	height: 76px;
}

#search form {
	float: right;
	margin: 0;
	padding: 25px 20px 0 0;
}

#search fieldset {
	margin: 0;
	padding: 0;
	border: none;
}

#search input {
	float: left;
	font: 12px Georgia, "Times New Roman", Times, serif;
	border: none;
}

#search-text {
	width: 135px;
	height: 18px;
	padding: 3px 0 0 5px;
	background: #ECF9E4;
	color: #658453;
}

#search-submit {
	height: 21px;
	margin-left: 12px;
	color: #39561D;
}
/* Page */
#page {
	width: 900px;
	margin: 0 auto;
	padding: 30px 0px 0px 0px;
}
/** LOGO */
#logo {
	width: 900px;
	height: 200px;
	margin: 0 auto;
	background: url(images/img-banner2.png) no-repeat left top;
}

#logo h1,#logo p {
	margin: 0;
	padding: 0 0 0 170px;
	line-height: normal;
}

#logo h1 {
	float: left;
	padding-top: 70px;
}

#logo h1 a {
	text-decoration: none;
	text-transform: lowercase;
	color: #FFFFFF;
}

#logo h1 a:hover {
	text-decoration: underline;
}

#logo p {
	padding: 130px 0 0 170px;
	font: italic 13px Georgia, "Times New Roman", Times, serif;
	color: #FFFFFF;
}

#logo p a {
	text-decoration: none;
	color: #FFFFFF;
}

#logo p a:hover {
	text-decoration: underline;
}

#logo-login {
	width: 900px;
	height: 200px;
	margin: 0 auto;
	background: url(images/img-banner-login.png) no-repeat left top;
}
/* Content */
#content {
	float: right;
	width: 680px;
}
/* Post */
.post {
	margin-bottom: 25px;
}

.post .title {
	height: 30px;
	margin-bottom: 3px;
	padding: 6px 0px 2px 25px;
	/*background: url(images/img05.jpg) no-repeat left top;*/
	color: #ffffff;
	background-color: #006666;
	border-radius: 5px;
}

.post .title-content {
	height: 30px;
	margin-bottom: -3px;
	padding: 5px 0px 0px 0px;
	color: #ffffff;
	background-color: #006666;
	border-radius: 5px 5px 0px 0px;
	width: 226px;
	text-align: center;
	font-size: 15px;
}

.post .poition {
	float: right;
	margin-top: -30px;
	padding-right: 20px;
	font-weight: normal;
	font-size: 16px;
	color: #ffffff;
}

.post .option {
	float: right;
	margin-top: -35px;
	/* width: 87px; */
	margin-right: 10px;
	/* padding-right: 20px; */
	font-weight: normal;
	font-size: 18px;
	color: #000000;
	border: 1px solid #ffffff;
	border-radius: 5px 5px 5px 5px;
}

#screensize {
	border: 1px solid #ffffff;
	border-radius: 5px 5px 5px 5px;
}

.post .meta {
	margin: -30px 0 3px 25px;
	padding: 2px 30px 2px 0px;
	font-family: Arial, Helvetica, sans-serif;
	font-weight: normal;
	font-size: 10px;
	color: #B6ACA2;
}

.post .meta a {
	color: #B6ACA2;
}

.post .entry {
	padding: 15px 10px 0px 10px
}

.post .links {
	margin: 0 250px 0 0;
	padding: 0 0 0 0px;
}

.post .links .comments {
	
}

.post .links .permalink {
	padding-left: 17px;
}
/* Sidebar */
#sidebar .option-group {
	margin-top: 0px;
	/* width: 87px; */
	margin-right: 10px;
	/* padding-right: 20px; */
	font-weight: normal;
	font-size: 18px;
	color: #000000;
	border: 1px solid #006666;
}

#selectgroup {
	width: 198px;
	height: 25px;
	padding: 0px 0px 0px 5px;
	font-size: 15px;
}
#selectsubject {
	width: 198px;
	height: 25px;
	padding: 0px 0px 0px 5px;
	font-size: 15px;
}

#sidebar {
	float: left;
	width: 200px;
}

#sidebar ul {
	margin: 0;
	padding: 0;
	list-style: none;
	line-height: normal;
}

#sidebar li {
	margin-bottom: 30px;
	padding: 0 0 20px 0px;
}

#sidebar li ul {
	line-height: 200%;
}

#sidebar li li {
	margin: 0;
	padding: 0 20px;
}

#sidebar p {
	margin: 0;
	padding: 0 20px;
}
/*#sidebar h2 {
	height: 22px;
	margin: 0 0 0px 0px;
	padding: 8px 0 2px 40px;
	background: url(images/img04.jpg) no-repeat left top;
	font-size: 20px;
	color: #FFFFFF;
}*/
#sidebar h2 {
	height: 25px;
	margin: 0 0 0px 0px;
	padding: 4px 0 6px 0px;
	background-color: #006666;
	font-size: 18px;
	color: #FFFFFF;
	border-radius: 6px 6px 0px 0px;
	text-align: center;
}

#sidebar p {
	line-height: 200%;
}
/* Calendar */
#calendar {
	
}

#calendar caption {
	padding-bottom: 5px;
	font-weight: bold;
}

#calendar table {
	width: 100%;
	border-collapse: collapse;
	border-right: 1px solid #F5F2EF;
}

#calendar thead th {
	padding: 5px 0;
	text-align: center;
	border-top: 1px solid #F5F2EF;
	border-left: 1px solid #F5F2EF;
	background: #F5F2EF;
}

#calendar tbody td {
	padding: 5px 0;
	text-align: center;
	border-top: 1px solid #F5F2EF;
	border-left: 1px solid #F5F2EF;
}

#calendar tfoot td {
	padding: 5px;
}

#calendar tfoot #next {
	text-align: right;
}

#calendar #today {
	background: #E7F1E1;
}
/* Footer */
#footer {
	width: 900px;
	margin: 0 auto;
	padding: 0;
	/* background: url(images/img07.png) no-repeat left top; */
	height: 61px;
	border-radius: 12px;
	background: #006666;
	height: 50px;
}

#footer p {
	margin: 0;
	padding: 20px 0;
	text-align: center;
	line-height: normal;
	color: #ffffff;
}

#footer a {
	color: #F1B566;
}
/*Login style*/
section {
	width: 275px;
	background: #ecf0f1;
	padding: 30px 30px 30px 30px;
	margin: 20px auto;
	text-align: center;
	border-radius: 5px;
}

.login span {
	display: block;
	position: relative;
	margin: 0 auto;
	top: -40px;
	height: 80px;
	width: 80px;
/* 	background: */
/* 		url('images/a1.png') */
/* 		center center no-repeat, #ecf0f1; */
/* 	border-radius: 50%; */
/* 	box-shadow: 1px 1px 2px rgba(0, 0, 0, .3); */
}

.login h1 {
	font-size: 24px;
	font-weight: 100;
	margin-bottom: 30px;
	margin-top: -20px;
}

.login input {
	width: 100%;
	background: #ffffff;
	border: none;
	height: 30px;
	margin-bottom: 15px;
	border-radius: 5px;
	text-align: center;
	font-size: 14px;
	color: #000000;
}

.login input:focus {
	outline: none;
}

.login input[type="submit"] {
	width: 100%;
	height: 30px;
	border: none;
	background: #3498db;
	color: #ecf0f1;
	font-weight: 100;
	margin-bottom: 15px;
	border-radius: 5px;
	transition: all ease-in-out .2s;
	border-bottom: 3px solid #2980b9;
	cursor: pointer;
}

.login input[type="submit"]:focus {
	outline: none;
}

.login input[type="submit"]:hover {
	background: #2980b9;
}

.login h2 {
	font-size: .75em;
}

.login a {
	color: #e74c3c;
	text-decoration: none;
	transition: all ease-in-out .2s;
}

.login a:hover {
	color: #c0392b;
}

.login-error {
	color: red;
	font-size: 15px !important;
	text-align: center;
}

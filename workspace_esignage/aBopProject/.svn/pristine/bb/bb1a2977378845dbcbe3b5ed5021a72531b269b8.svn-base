/*-----Start Tabs -----*/
.tabs {
	width: 100%;
	display: inline-block;
}
/*----- Tab Links -----*/
/* Clearfix */
.tab-links:after {
	display: block;
	clear: both;
	content: '';
}

.tab-links li {
	margin: 0px 1px;
	float: left;
	list-style: none;
}

.tab-links a {
	padding: 9px 15px;
	display: inline-block;
	border-radius: 10px 10px 0px 0px;
	background: #006666;
	font-size: 16px;
	font-weight: 600;
	color: #FFF;
	text-decoration: none;
	transition: all linear 0.15s;
}

.tab-links a:hover {
	background: #006666;
	color: #000000;
	text-decoration: none;
}

li.active a,li.active a:hover {
	background: #666666;
	color: #ffffff;
}
/*----- Content of Tabs -----*/
.tab-content { /*padding: 5px; */
	border-radius: 3px;
	/* box-shadow: -1px 1px 1px rgba(0, 0, 0, 0.15); */
	background: #fff;
}

.tab {
	display: none;
}

.tab.active {
	display: block;
}
/*End Tabs*/

